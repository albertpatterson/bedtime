{
    "manifest_version": 2,
    "name": "bedtime",
    "description": "A simple chrome extension to remind the user of bedtime",
    "version": "1.0",
    "browser_action": {
        "default_icon": "bedtime.png"
    },
    "background": {
        "scripts":["background/background.js"]
      },
    "content_scripts": [
      { "matches": ["<all_urls>"],
        "js": ["content/content.js"],
        "css": []
      }]
  }